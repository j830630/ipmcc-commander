# IPMCC Commander Environment Configuration

# Database
DATABASE_URL=sqlite+aiosqlite:///./data/ipmcc.db

# API Settings
DEBUG=true
CORS_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]

# Charles Schwab API (required for real-time data and trading)
# Get these from https://developer.schwab.com
SCHWAB_APP_KEY=your_app_key_here
SCHWAB_APP_SECRET=your_app_secret_here
SCHWAB_CALLBACK_URL=https://127.0.0.1

# Finnhub API (optional - for economic calendar)
# Get from https://finnhub.io
FINNHUB_API_KEY=demo

# Feature flags
USE_SCHWAB_API=false  # Set to true to use Schwab for all market data
